    "release": "bumpp && pnpm publish",
    "prepublishOnly": "pnpm run build"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.1.1",
    "@types/node": "^22.15.17",
    "bumpp": "^10.1.0",
    "happy-dom": "^17.4.7",
    "prettier": "^3.5.3",
    "tsdown": "^0.11.9",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3",
    "vitest": "^3.1.3"
  },
  "prettier": "@sxzz/prettier-config"
