{
  "name": "deferred-cleanup-resource-map",
  "version": "0.1.0",
  "description": "A deferred cleanup resource map.",
  "type": "module",
  "license": "MIT",
  "homepage": "https://github.com/gugustinette/create-tsdown#readme",
  "bugs": {
    "url": "https://github.com/gugustinette/create-tsdown/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gugustinette/create-tsdown.git"
  },
  "author": "bgl gwyng",
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js",
    "./package.json": "./package.json"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "lint": "biome check .",
